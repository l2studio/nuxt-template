{
  "name": "@l2studio/nuxt-template",
  "version": "0.1.0",
  "description": "Koa-based Nuxt template for L2 Studio",
  "author": "lgou2w <lgou2w@hotmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "rimraf .nuxt",
    "build": "nuxt build",
    "dev": "npm run dev:nuxt",
    "dev:serve": "cross-env DEBUG='lgou2w:*' NODE_ENV=development BOOTSTRAP_MODE=serve nodemon --config nodemon.serve.json",
    "dev:nuxt": "cross-env DEBUG='lgou2w:*' NODE_ENV=development BOOTSTRAP_MODE=nuxt nodemon --config nodemon.nuxt.json",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register -T -P tsconfig.json server/index.ts",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@koa/router": "^10.0.0",
    "@nuxtjs/composition-api": "^0.24.2",
    "@nuxtjs/universal-storage": "^0.5.9",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "dotenv": "^10.0.0",
    "joi": "^17.4.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-response-time": "^2.1.0",
    "koa-session": "^6.2.0",
    "koa-static": "^5.0.0",
    "nuxt": "^2.15.6",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.6",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/koa": "^2.13.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-response-time": "^2.1.0",
    "@types/koa-session": "^5.10.3",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.4",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.2.6",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.10.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^4.3.0",
    "postcss-nested": "^5.0.5",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.2",
    "ts-loader": "^8.3.0",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.12"
  }
}
